<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/inner_layout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginBottom="5dp"
    android:layout_marginLeft="10dp"
    android:layout_weight="4"
    android:gravity="center_vertical"
    android:padding="5dp">


    <RelativeLayout
        android:id="@+id/relativeLayout11"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/clientes">

        <ImageView
            android:id="@+id/complet_image"
            android:layout_width="90dp"
            android:layout_height="90dp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:scaleType="fitCenter"
            android:src="@drawable/searchicon" />
    </RelativeLayout>

    <TextView
        android:id="@+id/cnpj"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/nome"
        android:layout_alignLeft="@+id/nome"
        android:layout_alignRight="@+id/nome"
        android:layout_alignStart="@+id/nome"
        android:layout_below="@+id/nome"
        android:paddingLeft="5dp"
        android:text="Ref"
        android:textAlignment="center"
        android:textColor="@color/CinzaEscuro"
        android:textSize="15dip" />

    <TextView
        android:id="@+id/nome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/relativeLayout11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/relativeLayout11"
        android:layout_alignTop="@+id/relativeLayout11"
        android:layout_marginTop="100dp"
        android:text="name"
        android:textAlignment="center"
        android:textColor="@color/CorBotaoEnviar"
        android:textSize="18dip"
        android:textStyle="bold"
        android:typeface="sans" />

</RelativeLayout>